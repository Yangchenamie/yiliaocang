
.con-ul ._li{
    height: 280rpx;
    padding: 40rpx 30rpx 30rpx;
    border-bottom: 10rpx solid #f5f5f5;
}
.con-ul ._li ._img{
  width: 100%;
  height: 200rpx;
}
.li-fl{
  width: 41%;
  float: left;
}
.li-fr{
  width: 59%;
  float: left;
  padding-left: 20rpx;
  box-sizing: border-box;
}
.li-fr .title{
  margin-top: 10rpx;
    color: #333;
    font-size: 32rpx;
    line-height: 42rpx;
}
.li-fr .much{
  margin: 20rpx 0;
    color: #999;
    font-size: 24rpx;
}
.li-fr .rank{
  border-radius: 10rpx;
  width: 63%;
  height: 60rpx;
  background-color: #8abeff;
  font-size: 24rpx;
  line-height: 60rpx;
  color: #fff;
}
.rank .rank-fl{
  width: 35%;
  float: left;
  text-align: center;
  background-color: #177fff;
  border-top-left-radius: 10rpx;
  border-bottom-left-radius: 10rpx;
}
.rank .rank-fr{
  float: left;
  width: 65%;
  text-align: center;
}
.li-fr .free{
  color: #f9284d;
  font-size: 28rpx;
  margin-top: 10rpx;
}

